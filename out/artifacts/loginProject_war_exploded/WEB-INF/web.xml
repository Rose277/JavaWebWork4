<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Download</servlet-name>
        <servlet-class>com.xxxx.controller.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Download</servlet-name>
        <url-pattern>/Download</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Downloadmiddle</servlet-name>
        <servlet-class>com.xxxx.controller.Downloadmiddle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Downloadmiddle</servlet-name>
        <url-pattern>/Downloadmiddle</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DownloadTop</servlet-name>
        <servlet-class>com.xxxx.controller.DownloadTop</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadTop</servlet-name>
        <url-pattern>/DownloadTop</url-pattern>
    </servlet-mapping>
</web-app>